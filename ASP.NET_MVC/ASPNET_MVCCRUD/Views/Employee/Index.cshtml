@model List<ASPNET_MVCCRUD.Models.Domain.MyEmployee>

@{
}

<h1>Employees</h1>

<table class= "table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
            <th>Salary/th>
            <th>Date of Birth</th>
            <th>Department</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var emp in Model)
        {
            <tr>
                <td>@emp.Id</td>
                <td>@emp.Name</td>
                <td>@emp.Email</td>
                <td>@emp.Salary</td>
                <td>@emp.DateOfBirth.ToString("dd-MM-yyyy")</td>
                <td>@emp.Department</td>
                <td><a href = "Employee/View/@emp.Id">View</a></td>
            </tr>
        }
    </tbody>
</table>
